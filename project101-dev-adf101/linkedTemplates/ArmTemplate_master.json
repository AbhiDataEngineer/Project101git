{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "project101-dev-adf101"
		},
		"LS_AzureBlobStorage_Dev_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Dev-SA-Access-Secret"
		},
		"LS_AzureKeyVault_Dev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://project101-dev-kv101.vault.azure.net/"
		},
		"trigger1evnt_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/96a67257-0c9c-4fc1-ab5d-f909fd7c9c72/resourceGroups/Project101-Dev-RG/providers/Microsoft.Storage/storageAccounts/project101devsa"
		},
		"event_trigger_properties_copydata_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"event_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/96a67257-0c9c-4fc1-ab5d-f909fd7c9c72/resourceGroups/Project101-Dev-RG/providers/Microsoft.Storage/storageAccounts/project101devsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "project101-dev-adf101_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureKeyVault_Dev_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_Dev_properties_typeProperties_baseUrl')]"
					},
					"trigger1evnt_properties_typeProperties_scope": {
						"value": "[parameters('trigger1evnt_properties_typeProperties_scope')]"
					},
					"LS_AzureBlobStorage_Dev_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_AzureBlobStorage_Dev_properties_typeProperties_connectionString_secretName')]"
					},
					"event_trigger_properties_copydata_parameters_FileName": {
						"value": "[parameters('event_trigger_properties_copydata_parameters_FileName')]"
					},
					"event_trigger_properties_typeProperties_scope": {
						"value": "[parameters('event_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}